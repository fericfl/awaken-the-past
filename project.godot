"Localnic": "res://Dialog/Localnic.dch",
"ceaser": "res://Dialog/ceaser.dch",
"faraon": "res://Dialog/faraon.dch",
"priest": "res://Assets/Backgrounds/priest.dch",
"socrate": "res://Dialog/socrate.dch",
"teacher": "res://Dialog/teacher.dch"
}
